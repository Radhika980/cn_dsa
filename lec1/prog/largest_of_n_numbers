import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
      Scanner sc=new Scanner(System.in);
    int n = sc.nextInt ();
    int max=-100000000;
    for(int i=0;i<n;i++)
    {
         int k=sc.nextInt();
         if(k>max)max=k;
        
    }

    System.out.println ("max-"+max);
    
  }
}
